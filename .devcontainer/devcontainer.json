{
  "name": "Spring Boot + MySQL Dev Container",
  "dockerComposeFile": "docker-compose.yml",
  "runServices": ["mysql", "app"],
  "service": "app",
  "workspaceFolder": "/workspace/turbo-potato",
  "forwardPorts": [8080, 3306],
  "waitFor": "onCreateCommand",
  "postCreateCommand": "chmod +x mvnw reset.sh install_extensions.sh",
  "postAttachCommand": {
    "server": "./mvnw spring-boot:run"
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "src/main/java/com/example/adam/AdamApplication.java"
      ]
    },
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-spring-boot",
        "ms-azuretools.vscode-docker",
        "ms-vsliveshare.vsliveshare"
      ]
    }
  },
  "portsAttributes": {
    "8080": {
      "label": "Swagger UI",
      "onAutoForward": "openPreview",
      "protocol": "http",
      "path": "/swagger"
    }
  }
}
