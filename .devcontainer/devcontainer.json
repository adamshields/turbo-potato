{
  "name": "Spring Boot + MySQL Dev Container",
  "dockerComposeFile": "docker-compose.yml",
  "runServices": ["mysql", "app"],
  "service": "app",
  "workspaceFolder": "/workspace/turbo-potato",
  "forwardPorts": [8080, 3306],
  "postCreateCommand": "chmod +x mvnw",
  "postStartCommand": "./mvnw spring-boot:run",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-spring-boot"
      ]
    }
  },
  "portsAttributes": {
    "8080": {
      "label": "Swagger UI",
      "onAutoForward": "openBrowser"
    }
  }
}
